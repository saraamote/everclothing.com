}

{% if settings.menu_top_padding %}
  .nav ul.menu { padding-top: {{ settings.menu_top_padding }}px; }
{% endif %}

/* sub-levels link */
.menu ul a, .menu li:hover > ul a, .index .header .nav ul.menu ul li a  {
  white-space: nowrap;
  padding: 8px 0 8px 15px;
  background: none;
